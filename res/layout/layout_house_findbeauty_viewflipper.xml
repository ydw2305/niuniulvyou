<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout1"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:gravity="center_vertical" >

    <ImageView
        android:id="@+id/iv_layout_house_findbeauty_viewfipper"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_margin="@dimen/m_out"
        android:src="@drawable/ic_empty" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_left_quote" />

    <TextView
        android:id="@+id/tv_layout_house_findbeauty_viewfipper"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/m_normal"
        android:textSize="@dimen/ts_small"
        android:textColor="@color/color_grey"
        android:maxEms="4"
        android:singleLine="true" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/icon_right_quote" />

</LinearLayout>
