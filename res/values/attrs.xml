<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="text" format="string" />

    <declare-styleable name="CustomIndictor">
        <attr name="radius" format="integer" />
        <attr name="strokewidth" format="integer" />
        <attr name="SrcColor" format="reference|color" />
        <attr name="BackGroundColor" format="reference|color" />
    </declare-styleable>
    
    <declare-styleable name="CustomIndictorCircle">
        <attr name="cradius" format="integer" />
        <attr name="cSrcColor" format="reference|color" />
        <attr name="cBackGroundColor" format="reference|color" />
    </declare-styleable>
    
    <declare-styleable name="letterBarCustom">
        <attr name="bottom" format="integer" />
        <attr name="textSize" format="integer" />
        <attr name="touchColor" format="reference|color" />
        <attr name="moColor" format="reference|color" />
    </declare-styleable>

</resources>